    </matches>
    <textblocks>
	  <link block="inv_name" separator="">
	   <property name="Unique" />
	   <property name="Name" />
	  </link>
	  <link block="inv_willpower">
	   <property name="Willpower" />
	  </link>
	  <link block="inv_intellect">
	   <property name="Intellect" />
	  </link>
	  <link block="inv_combat">
	   <property name="Combat" />
	  </link>
	  <link block="inv_agility">
	   <property name="Agility" />
	  </link>
	  <link block="inv_traits">
	   <property name="Traits" />
	  </link>
	  <link block="inv_description" separator="&#10;&#10;">
	   <property name="Keywords" />
	   <property name="Text" />
	  </link>
	  <link block="inv_health">
	   <property name="Health" />
	  </link>
	  <link block="inv_sanity">
	   <property name="Sanity" />
	  </link>
    </textblocks>
  </template>
  <template src="proxy/asset-neutral.jpg" default="False">
